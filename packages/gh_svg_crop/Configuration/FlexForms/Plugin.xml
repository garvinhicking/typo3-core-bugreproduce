<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>Optionen</sheetTitle>
                <type>array</type>
                <el>
                    <settings.cropImage>
                        <label>Crop Image(s)</label>
                        <config>
                            <type>file</type>
                            <maxitems>99</maxitems>
                            <allowed>common-image-types</allowed>
                        </config>
                    </settings.cropImage>
                    <settings.showDemo>
                        <label>Enable showing cropping demos</label>
                        <config>
                            <type>check</type>
                            <renderType>checkboxToggle</renderType>
                            <numIndex index="0" type="array">
                                <label>Enable</label>
                                <value>1</value>
                            </numIndex>
                        </config>
                    </settings.showDemo>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
